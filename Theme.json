{
    "Fonts": {
        "primaryFontLight":"BrandonGrotesque-Light",
        "primaryFontMedium":"BrandonGrotesque-Medium",
        "primaryFontBold":"BrandonGrotesque-Bold",
        "primaryFontBlack":"BrandonGrotesque-Black",
        "primaryFontLightItalic":"BrandonGrotesque-LightItalic"
    },
    "Colors": {
        "primaryColor": {
            "red": 1,
            "green": 1,
            "blue": 1,
            "alpha": 1
        },
        "secondaryColor": {
          "red": 0,
          "green": 0,
          "blue": 0,
          "alpha": 1
        }
    },
    "Images": {
      "buttonImage1": "black_button_image"
    },
    "Labels": {
        "H1": {
            "font": "primaryFontLight",
            "size": "34",
            "textColor": "primaryColor"
        },
        "H2": {
          "font": "primaryFontLightItalic",
          "size": "20",
          "textColor": "secondaryColor"
        },
        "H3": {
          "font": "primaryFontBlack",
          "size": "24"
        }
    },
    "Buttons": {
          "B1": {
            "titleLabelFont": "primaryFontBold",
            "size": "34",
            "backgroundColor": "secondaryColor",
            "cornerRadius": 22,
            "normal": {
                "titleColor": "primaryColor",
                "titleShadowColor" : "primaryColor"
            },
            "highlighted": {
                "titleColor": "secondaryColor"
            }
        },
          "B2": {
            "titleLabelFont": "primaryFontBold",
            "size": "24",
            "backgroundColor": "primaryColor",
            "cornerRadius": 8,
            "normal": {
                "titleColor": "secondaryColor",
                "titleShadowColor" : "primaryColor"
            }
        },
         "B3": {
             "titleLabelFont": "primaryFontMedium",
             "size": "22",
             "borderColor": "primaryColor",
             "borderWidth": 2,
             "normal": {
                 "titleColor": "primaryColor"
             }
         },
         "B4": {
            "normal": {
                "backgroundImage": "buttonImage1"
              }
          }
      },
      "TextFields": {
          "T1": {
              "textColor": "primaryColor",
              "backgroundColor": "secondaryColor",
              "borderColor": "primaryColor",
              "borderWidth": 1,
              "cornerRadius": 8
          }
      }
}
